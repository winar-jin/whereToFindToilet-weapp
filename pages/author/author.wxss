.view {
  text-align: center;
  height: calc(100vh);
  width: calc(100vw);
  background-color: #f9f9f9;
}

/*.logo*/

.logo {
  width: 200rpx;
  height: 200rpx;
  margin-top: 70rpx;
}

/*版本信息*/

.version {
  font-size: 30rpx;
  margin-top: 10rpx;
}

.title {
  margin-bottom: 80rpx;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.zan-cell__bd {
  text-align: left;
}

.zan-cell:active {
  background-color: #f9f9f9;
}

.zan-cell__bd {
  font-weight: bold;
}

.zan-cell__ft {
  font-size: 26rpx;
  text-align: left;
}
